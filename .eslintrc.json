{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-one-expression-per-line": [{ "allow": "single-child" }],
    "react/jsx-filename-extension": "off",
    "no-underscore-dangle": [2, { "allow": ["_id"]}]
  }
}